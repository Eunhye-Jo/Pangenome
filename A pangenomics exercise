(anvio-8) foodlabui-iMac:V_jascida_genomes foodlab$ for g in `cat genomes.txt`
> do
>     echo
>     echo "Working on $g ..."
>     echo
>     anvi-gen-contigs-database -f ${g}_scaffolds_2.5K.fasta \
>                               -o V_jascida_${g}.db \
>                               --num-threads 4 \
>                               -n V_jascida_${g}
> done

