#### (2) FASTA files to Contigs DBs ####

(anvio-8) foodlabui-iMac:V_jascida_genomes foodlab$ ls *fasta | awk 'BEGIN{FS="_"}{print $1}' > genomes.txt
(anvio-8) foodlabui-iMac:V_jascida_genomes foodlab$ cat genomes.txt 
12
13
14
47
52
53
55
ref
(anvio-8) foodlabui-iMac:V_jascida_genomes foodlab$ for g in `cat genomes.txt`
> do
>     echo
>     echo "Working on $g ..."
>     echo
>     anvi-script-reformat-fasta ${g}_scaffolds.fasta \
>                                --min-len 2500 \
>                                --simplify-names \
>                                -o ${g}_scaffolds_2.5K.fasta
> done

