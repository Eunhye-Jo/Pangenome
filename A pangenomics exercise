(anvio-8) foodlabui-iMac:V_jascida_genomes foodlab$ for g in *.db
> do
>     anvi-run-hmms -c $g --num-threads 4
>     anvi-run-ncbi-cogs -c $g --num-threads 4
>     anvi-scan-trnas -c $g --num-threads 4
>     anvi-run-scg-taxonomy -c $g --num-threads 4
> done


