## Finalizing the FASTA files

(base) foodlabui-iMac:10_pangenome foodlab$ conda activate anvio-8
(anvio-8) foodlabui-iMac:10_pangenome foodlab$ 

(anvio-8) foodlabui-iMac:10_pangenome foodlab$ for g in `cat genomes.txt`
do     
  echo
  echo "Working on $g ..."
  echo
  anvi-script-reformat-fasta ${g}.fasta --min-len 2500 --simplify-names -o ${g}_2.5K.fasta
done

