## Calculating average nucleotide identity between genomes

anvi-compute-genome-similarity --external-genomes external-genomes-final.txt \
                               --program pyANI \
                               --output-dir ANI \
                               --num-threads 6 \
                               --pan-db Bv/Bv-PAN.db

(anvio-8) foodlabui-iMac:10_pangenome foodlab$ 
(anvio-8) foodlabui-iMac:10_pangenome foodlab$ anvi-compute-genome-similarity --external-genomes external-genomes-final.txt \
>                                --program pyANI \
>                                --output-dir ANI \
>                                --num-threads 6 \
>                                --pan-db Bv/Bv-PAN.db

