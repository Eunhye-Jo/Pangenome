(anvio-8) foodlabui-iMac:10_pangenome foodlab$ 

## Generating contigs databases


for g in `cat genomes.txt`
do
    echo
    echo "Working on $g ..."
    echo
    anvi-gen-contigs-database -f ${g}_2.5K.fasta \
                              -o ${g}.db \
                              --num-threads 4 \
                              -n ${g}
done


(anvio-8) foodlabui-iMac:10_pangenome foodlab$ for g in `cat genomes.txt`; do     echo;     echo "Working on $g ...";     echo;     anvi-script-reformat-fasta ${g}.fasta                                --min-len 2500                                --simplify-names                                -o ${g}_2.5K.fasta; done

